[Rewrite]
# 开屏广告
^https:\/\/appapi\.51job\.com\/api\/market\/get_launch\.php\? - reject-dict
# 职位 - 下拉二楼
^https:\/\/cupid\.51job(app)?\.com\/open\/51job-activities\/secJob\/queryHomeSecondConfigV2\? - reject-dict
# 职位 - 职位详情 - 竞争力分析
^https:\/\/cupid\.51job(app)?\.com\/open\/noauth\/jobs\/detail\/sesame-competitive\/ - reject-dict
# 简历向导
^https:\/\/cupid\.51jobapp(app)?\.com\/open\/resume\/strategy\/resume-build\? - reject-dict
# 弹窗
^https:\/\/cupid\.51job(app)?\.com\/open\/noauth\/popUp\/ - reject-dict
# 消息页面 - 无忧小助手 - 横幅推广
^https:\/\/cupid\.51job(app)?\.com\/open\/operation\/get\/banner-list\? - reject-dict

[Script]
http-response ^https:\/\/cupid\.51job(app)?\.com\/open\/noauth\/recommend\/job-tab-dynamic\? script-path=https://raw.githubusercontent.com/mist-whisper/Script-Hub/refs/heads/master/scripts/body-rewrite.js, requires-body=true, timeout=30, tag=body_rewrite_17, argument=%5B%5B%22json-del%22%2C%5B%22resultbody.adsTabFeeds%22%5D%5D%5D

http-response ^https:\/\/cupid\.51job(app)?\.com\/open\/user-task\/user\/task\/ script-path=https://raw.githubusercontent.com/mist-whisper/Script-Hub/refs/heads/master/scripts/body-rewrite.js, requires-body=true, timeout=30, tag=body_rewrite_21, argument=%5B%5B%22json-del%22%2C%5B%22resultbody.availableTaskList%22%5D%5D%5D

http-response ^https:\/\/cupid\.51job(app)?\.com\/open\/tab\/good-job\/modules script-path=https://raw.githubusercontent.com/mist-whisper/Script-Hub/refs/heads/master/scripts/body-rewrite.js, requires-body=true, timeout=30, tag=body_rewrite_27, argument=%5B%5B%22json-del%22%2C%5B%22resultbody.bannerList%22%5D%5D%5D

[MITM]
hostname = appapi.51job.com, cupid.51job.com, cupid.51jobapp.com
